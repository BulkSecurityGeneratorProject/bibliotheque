<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="org.miage.m2sid.bibliotheque.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="org.miage.m2sid.bibliotheque.domain.Usager"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Usager.reservations"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Usager.emprunts"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Emprunt"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Reservation"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Oeuvre"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Oeuvre.reservations"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Oeuvre.exemplaires"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Livre"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Livre.auteurs"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Magazine"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Exemplaire"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Exemplaire.emprunts"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Auteur"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="org.miage.m2sid.bibliotheque.domain.Auteur.livres"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
